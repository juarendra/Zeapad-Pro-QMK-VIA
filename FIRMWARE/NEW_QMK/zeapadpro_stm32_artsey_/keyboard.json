{
    "keyboard_name": "zeapad",
    "manufacturer": "Positron Electronic",
    "url": "https://github.com/",
    "maintainer": "Positron Electronic",
    "usb": {
        "vid": "0x3454",
        "pid": "0x5445",
        "device_version": "0.0.1"
    },
    "encoder": {
        "rotary": [
            {"pin_a": "C13", "pin_b": "C14", "resolution": 4},
            {"pin_a": "B12", "pin_b": "B8", "resolution": 4},
            {"pin_a": "B15", "pin_b": "B13", "resolution": 4}
        ]
    },
    "rgblight": {
        "driver": "ws2812",
        "animations": {
            "alternating": false,
            "breathing": true,
            "christmas": true,
            "knight": true,
            "rainbow_mood": true,
            "rainbow_swirl": true,
            "rgb_test": false,
            "snake": true,
            "static_gradient": true,
            "twinkle": true
        },
        "default": {
            "hue": 213
        },
        "brightness_steps": 8,
        "led_count": 16,
        "max_brightness": 150,
        "saturation_steps": 8,
        "sleep": true
    },
    "ws2812": {
        "pin": "B14"
    },
    "layouts": {
        "LAYOUT_ortho_4x5": {
            "layout": [
                { "matrix": [0, 0], "x": 0, "y": 0 },
                { "matrix": [0, 1], "x": 1, "y": 0 },
                { "matrix": [0, 2], "x": 2, "y": 0 },
                { "matrix": [0, 3], "x": 3, "y": 0 },
